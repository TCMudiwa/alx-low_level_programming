#include <stdio.h>
/**
* main - prints alphabet in lowercase
* Return: 0 if successful
**/
int main(void)
{
int alphabet, j;
for (alphabet = '0'; alphabet <= '9'; alphabet++)
{
for (j = '0' ; j <= '9' ; j++)
{
if ((alphabet <= j) & (j <= '9'))
{
putchar(alphabet);
putchar(j);
if ((j < '9') | (alphabet <= '8'))
{
putchar(',');
putchar(' ');
}
}
}
}
putchar('\n');
return (0);
}
